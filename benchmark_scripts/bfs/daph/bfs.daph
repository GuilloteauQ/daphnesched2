start_reading = now();
G = readMatrix($f);
start_compute=now();
maxi = 200;
n = nrow(G);
x = fill(0.0, n, 1);
x[0,0] = [1.0];
one = fill(1.0, n, 1);

for(iter in 1:maxi) {
  x = min(one, x + G @ x);
}
end=now();
